#include <bits/stdc++.h>
using namespace std;
int main() {
	// your code goes here
	vector<int>v={2,3,5,6,7};
	cout<<"for printing the vectors\n";
	for(int i=0;i<v.size();++i)
	{
	    cout<<v[i]<<" ";
	}
	cout<<endl;
	vector<pair<int,int>>v_p={{1,2},{2,3},{3,4}};
	vector<pair<int,int>>::iterator it;
	cout<<"for printing using the iterator (*it)\n";
	for(it=v_p.begin();it!=v_p.end();it++)
	{
	    cout<<(*it).first<<" "<<(*it).second<<endl;
	}
	/*
	(*it).first<===> (it->first) 
	both denotes the same statement for use.
	usually prefer the second statement for the use.
	because it is easy to use.
	*/
	cout<<"for printing using the iterator the (it->first)\n";
    for(it=v_p.begin();it!=v_p.end();it++)
	{
	    cout<<(it->first)<<" "<<(it->second)<<endl;
	}
	
}
