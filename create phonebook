#include<bits/stdc++.h>
using namespace std;
int main()
{
    map<string,list<string>>phonebook;
    phonebook["prateek"].push_back("11111111");
    phonebook["prateek"].push_back("11111112");
    phonebook["prateek"].push_back("11111113");
      
    phonebook["krishna"].push_back("23111111");
    phonebook["krishna"].push_back("24111112");
    phonebook["krishna"].push_back("25111111");
    
    //....try to make a phone book and print it
    for(auto &k:phonebook){
    cout<<k.first;
    for(auto i:k.second)
    cout<<i<<"\n";
    }
}
