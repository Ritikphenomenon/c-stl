#include <bits/stdc++.h>
using namespace std;
void printvec(vector<pair<int,int>>&v)
{
    cout<<"size ="<<v.size()<<endl;
    for(int i=0;i<v.size();i++)
    {
        cout<<v[i].first<<" "<<v[i].second<<endl;
    }
    cout<<endl;
}
int main() {

    //vectors of pairs.
	// your code goes here
	vector<pair<int,int>>v;//it is avector in which we have a pair of int data type.
	v={{1,2},{2,3},{4,5}};
	printvec(v);
	int n;
	cin>>n;
	for(int i=0;i<n;i++)
	{
	    int x,y;
	    cin>>x>>y;
	    v.push_back({x,y});//for taking the inputs.
	}
	cout<<"new input\n";
	printvec(v);
	
	
	 
	return 0;
}
